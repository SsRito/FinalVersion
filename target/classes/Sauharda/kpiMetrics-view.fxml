<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/22" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Sauharda.KpiMetricsController">
   <children>
      <Label prefHeight="69.0" prefWidth="606.0" style="-fx-background-color: #80cbc4;" text="      Key Performance Indicators (KPI)" textFill="WHITE">
         <font>
            <Font name="System Bold" size="33.0" />
         </font>
      </Label>
      <Button layoutX="225.0" layoutY="349.0" mnemonicParsing="false" onAction="#goBackToDashboardOnAction" style="-fx-border-color: white; -fx-background-color: #b2dfdb;" text="&lt;- Go Back to Dashboard" />
      <Button layoutX="37.0" layoutY="97.0" mnemonicParsing="false" onAction="#policyComplianceReportOnAction" prefHeight="219.0" prefWidth="90.0" style="-fx-background-color: #b2dfdb; -fx-border-color: white;" text="Policy Compliance Report" textAlignment="CENTER" wrapText="true" />
      <Button layoutX="147.0" layoutY="97.0" mnemonicParsing="false" onAction="#projectCompletionOnAction" prefHeight="219.0" prefWidth="90.0" style="-fx-background-color: #b2dfdb; -fx-border-color: white;" text="Project Completion" textAlignment="CENTER" wrapText="true" />
      <Button layoutX="256.0" layoutY="97.0" mnemonicParsing="false" onAction="#trainingInfoOnAction" prefHeight="219.0" prefWidth="90.0" style="-fx-background-color: #b2dfdb; -fx-border-color: white;" text="Training Info" textAlignment="CENTER" wrapText="true" />
      <Button layoutX="370.0" layoutY="97.0" mnemonicParsing="false" onAction="#employeePerformanceOnAction" prefHeight="219.0" prefWidth="90.0" style="-fx-background-color: #b2dfdb; -fx-border-color: white;" text="Employee Performance" textAlignment="CENTER" wrapText="true" />
      <Button layoutX="480.0" layoutY="97.0" mnemonicParsing="false" onAction="#volunteerReportOnAction" prefHeight="219.0" prefWidth="90.0" style="-fx-background-color: #b2dfdb; -fx-border-color: white;" text="Volunteer Report" textAlignment="CENTER" wrapText="true" />
   </children>
</AnchorPane>
