<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>


<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/22" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Sauharda.VolunteerReportView">
   <children>
      <Label prefHeight="69.0" prefWidth="606.0" style="-fx-background-color: #80cbc4;" text="      Volunteer Report" textFill="WHITE">
         <font>
            <Font name="System Bold" size="33.0" />
         </font>
      </Label>
      <Button layoutX="40.0" layoutY="344.0" mnemonicParsing="false" onAction="#goBackToDashboardOnAction" style="-fx-border-color: white; -fx-background-color: #b2dfdb;" text="&lt;- Go Back to Dashboard" />
      <Button layoutX="40.0" layoutY="86.0" mnemonicParsing="false" onAction="#loadReportOnAction" prefHeight="28.0" prefWidth="527.0" style="-fx-border-color: white; -fx-background-color: #b2dfdb;" text="Load Report" />
      <Button layoutX="409.0" layoutY="344.0" mnemonicParsing="false" onAction="#goBackToKPIPageOnAction" prefHeight="28.0" prefWidth="158.0" style="-fx-border-color: white; -fx-background-color: #b2dfdb;" text="&lt;- Go Back to KPI Page" />
      <TableView fx:id="volunteerReportTableView" layoutX="40.0" layoutY="133.0" prefHeight="190.0" prefWidth="527.0">
         <columns>
            <TableColumn fx:id="nameTableColumn" prefWidth="264.0" style="-fx-background-color: #b2dfdb; -fx-border-color: white;" text="Name" />
            <TableColumn fx:id="volunteeringFieldTableColumn" prefWidth="262.0" style="-fx-background-color: #b2dfdb; -fx-border-color: white;" text="Volunteering Field" />
         </columns>
      </TableView>
   </children>
</AnchorPane>
