<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>


<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/22" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Sauharda.JobRecruitmentController">
   <children>
      <Label prefHeight="69.0" prefWidth="606.0" style="-fx-background-color: #80cbc4;" text="      Recruit New Employee" textFill="WHITE">
         <font>
            <Font name="System Bold" size="33.0" />
         </font>
      </Label>
      <Button layoutX="40.0" layoutY="347.0" mnemonicParsing="false" onAction="#goBackToDashboardOnAction" style="-fx-border-color: white; -fx-background-color: #b2dfdb;" text="&lt;- Go Back to Dashboard" />
      <Button layoutX="373.0" layoutY="347.0" mnemonicParsing="false" onAction="#goBackToRecruitmentPageOnAction" style="-fx-border-color: white; -fx-background-color: #b2dfdb;" text="&lt;- Go Back to Recruitment Page" />
      <Label layoutX="40.0" layoutY="84.0" text="View Application:">
         <font>
            <Font name="System Bold" size="13.0" />
         </font>
      </Label>
      <Button layoutX="40.0" layoutY="104.0" mnemonicParsing="false" onAction="#loadApplicationOnAction" prefHeight="28.0" prefWidth="527.0" style="-fx-border-color: white; -fx-background-color: #b2dfdb;" text="Load Application" />
      <TableView fx:id="newEmployeeTableView" layoutX="40.0" layoutY="140.0" prefHeight="188.0" prefWidth="527.0">
         <columns>
            <TableColumn fx:id="nameTableColumn" prefWidth="153.0" style="-fx-background-color: #b2dfdb; -fx-border-color: white;" text="Name" />
            <TableColumn fx:id="dobTableColumn" prefWidth="119.0" style="-fx-background-color: #b2dfdb; -fx-border-color: white;" text="Date of Birth" />
            <TableColumn fx:id="genderTableColumn" prefWidth="108.0" style="-fx-background-color: #b2dfdb; -fx-border-color: white;" text="Gender" />
            <TableColumn fx:id="addressTableColumn" prefWidth="146.0" style="-fx-background-color: #b2dfdb; -fx-border-color: white;" text="Address" />
         </columns>
      </TableView>
   </children>
</AnchorPane>
