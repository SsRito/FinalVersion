<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/22" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Sauharda.PlanningController">
   <children>
      <Label prefHeight="69.0" prefWidth="606.0" style="-fx-background-color: #80cbc4;" text="      Planning" textFill="WHITE">
         <font>
            <Font name="System Bold" size="33.0" />
         </font>
      </Label>
      <Label layoutX="45.0" layoutY="85.0" text="New Goal" />
      <TextArea fx:id="newGoalTextAea" layoutX="45.0" layoutY="106.0" prefHeight="112.0" prefWidth="518.0" />
      <Button layoutX="265.0" layoutY="226.0" mnemonicParsing="false" onAction="#publishGoalOnAction" prefHeight="26.0" prefWidth="76.0" style="-fx-background-color: #b2dfdb; -fx-border-color: white;" text="Publish" />
      <Button layoutX="45.0" layoutY="333.0" mnemonicParsing="false" onAction="#goBackToDashboardOnAction" style="-fx-border-color: white; -fx-background-color: #b2dfdb;" text="&lt;- Go Back to Dashboard" />
      <Button layoutX="460.0" layoutY="333.0" mnemonicParsing="false" onAction="#viewKPIMetricsOnAction" style="-fx-border-color: white; -fx-background-color: #b2dfdb;" text="View KPI Metrics" />
   </children>
</AnchorPane>
