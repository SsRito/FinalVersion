         <font>
            <Font name="System Bold" size="33.0" />
         </font>
      </Label>
      <Button layoutX="40.0" layoutY="344.0" mnemonicParsing="false" onAction="#goBackToDashboardOnAction" style="-fx-border-color: white; -fx-background-color: #b2dfdb;" text="&lt;- Go Back to Dashboard" />
      <TableView fx:id="employeePerformanceTableView" layoutX="17.0" layoutY="125.0" prefHeight="188.0" prefWidth="574.0">
