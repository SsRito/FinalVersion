            <TableColumn fx:id="toDateTableColumn" prefWidth="145.0" style="-fx-background-color: #b2dfdb; -fx-border-color: white;" text="Date (to)" />
            <TableColumn fx:id="nameTableColumn" prefWidth="167.0" style="-fx-background-color: #b2dfdb; -fx-border-color: white;" text="Name" />
