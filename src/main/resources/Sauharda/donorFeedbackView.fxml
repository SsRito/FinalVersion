<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/22" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Sauharda.DonorFeedbackView">
   <children>
      <Label prefHeight="67.0" prefWidth="600.0" style="-fx-background-color: #80cbc4;" text="    Donor Feedback" textFill="WHITE">
         <font>
            <Font name="System Bold" size="30.0" />
         </font>
      </Label>
      <Button layoutX="39.0" layoutY="340.0" mnemonicParsing="false" onAction="#goBackToDashboardOnAction" style="-fx-background-color: #b2dfdb; -fx-border-color: white;" text="&lt;- Go Back to Dashboard" />
      <TableView fx:id="donorTableView" layoutX="40.0" layoutY="124.0" prefHeight="200.0" prefWidth="520.0">
         <columns>
            <TableColumn fx:id="donorNameTableColumn" prefWidth="146.0" style="-fx-background-color: #b2dfdb; -fx-border-color: white;" text="Name" />
            <TableColumn fx:id="donorFeedbackDateTableColumn" prefWidth="114.0" style="-fx-background-color: #b2dfdb; -fx-border-color: white;" text="Date" />
            <TableColumn fx:id="donorFeedbackTableColumn" prefWidth="259.0" style="-fx-background-color: #b2dfdb; -fx-border-color: white;" text="Feedback" />
         </columns>
      </TableView>
      <Button layoutX="407.0" layoutY="340.0" mnemonicParsing="false" onAction="#goBackToFeedbackOnAction" style="-fx-background-color: #b2dfdb; -fx-border-color: white;" text="&lt;- Go Back to Feedback" />
      <Button layoutX="253.0" layoutY="83.0" mnemonicParsing="false" onAction="#refreshTableOnAction" style="-fx-background-color: #b2dfdb; -fx-border-color: white;" text="Refresh Table" />
   </children>
</AnchorPane>
